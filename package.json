{
  "name": "paas",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:nginx\"",
    "dev:frontend": "vite dev --host --port 3000",
    "dev:nginx": "bash ./start-nginx.sh",
    "dev:backend": "concurrently --kill-others \"bun run --watch backend/server/main.ts\" \"npm run dev:backend:docker\"",
    "dev:backend:docker": "docker compose up",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "flowbite": "^2.4.1",
    "flowbite-svelte": "^0.46.15",
    "flowbite-svelte-icons": "^1.6.1",
    "globals": "^15.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "sass": "^1.77.8",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@trpc/client": "^11.0.0-rc.466",
    "@trpc/server": "^11.0.0-rc.466",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "drizzle": "^1.4.0",
    "drizzle-kit": "^0.23.0",
    "drizzle-orm": "^0.32.0",
    "js-yaml": "^4.1.0",
    "json-schema-library": "^9.3.5",
    "jsonschema": "^1.4.1",
    "pg": "^8.12.0",
    "zod": "^3.23.8"
  }
}
